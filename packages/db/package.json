{
  "name": "@ehentai-toplist-archive/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "drizzle-zod": "^0.8.3"
  },
  "peerDependencies": {
    "drizzle-orm": "^0.45.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260227.0"
  },
  "nx": {
    "sourceRoot": "packages/db/src",
    "projectType": "library",
    "tags": [],
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{projectRoot}/dist"
        ],
        "options": {
          "tsConfig": "packages/db/tsconfig.lib.json",
          "main": "packages/db/src/index.ts",
          "rootDir": "packages/db/src",
          "outputPath": "packages/db/dist",
          "generatePackageJson": false
        }
      }
    }
  }
}
